<?xml version="1.0"?>
<psalm
    errorLevel="1"
    phpVersion="8.2"
    resolveFromConfigFile="true"
    findUnusedPsalmSuppress="true"
    findUnusedBaselineEntry="true"
    totallyTyped="true"
>
    <projectFiles>
        <directory name="src" />
        <ignoreFiles>
            <directory name="tests" />
            <directory name="vendor" />
        </ignoreFiles>
    </projectFiles>
</psalm>
